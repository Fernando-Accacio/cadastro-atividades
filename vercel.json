{
  "version": 2,
  "builds": [
    {
      "src": "api/__init__.py", 
      "use": "@vercel/python"
    },
    {
      
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { 
        
        "distDir": "build" 
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/__init__.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(ico|png|jpg|jpeg|svg|gif|json))",
      "dest": "/$1"
    },
    {
       "src": "/documents/(.*)",
       "dest": "/documents/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}